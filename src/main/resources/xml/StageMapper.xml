<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cjrj.edu.mapper.StageMapper">
  <resultMap id="BaseResultMap" type="com.cjrj.edu.entity.Stage">
    <id column="STAGE_ID" jdbcType="DECIMAL" property="stageId" />
    <result column="STAGE_NAME" jdbcType="VARCHAR" property="stageName" />
    <result column="CREATEDATE" jdbcType="TIMESTAMP" property="createdate" />
    <result column="CREATENAME" jdbcType="VARCHAR" property="createname" />
    <result column="MODIFYDATE" jdbcType="TIMESTAMP" property="modifydate" />
    <result column="MODIFYNAME" jdbcType="VARCHAR" property="modifyname" />
    <result column="DEL_FLAG" jdbcType="DECIMAL" property="delFlag" />
  </resultMap>
  <sql id="Base_Column_List">
    STAGE_ID, STAGE_NAME, CREATEDATE, CREATENAME, MODIFYDATE, MODIFYNAME, DEL_FLAG
  </sql>
  <insert id="insertSelective" parameterType="com.cjrj.edu.entity.Stage">
    <selectKey keyProperty="stageId" order="BEFORE" resultType="java.math.BigDecimal">
      select stage_seq.nextval from dual
    </selectKey>
    insert into T_STAGE
    <trim prefix="(" suffix=")" suffixOverrides=",">
      STAGE_ID,
      <if test="stageName != null">
        STAGE_NAME,
      </if>
      <if test="createdate != null">
        CREATEDATE,
      </if>
      <if test="createname != null">
        CREATENAME,
      </if>
      <if test="modifydate != null">
        MODIFYDATE,
      </if>
      <if test="modifyname != null">
        MODIFYNAME,
      </if>
      <if test="delFlag != null">
        DEL_FLAG,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      #{stageId,jdbcType=DECIMAL},
      <if test="stageName != null">
        #{stageName,jdbcType=VARCHAR},
      </if>
      <if test="createdate != null">
        #{createdate,jdbcType=TIMESTAMP},
      </if>
      <if test="createname != null">
        #{createname,jdbcType=VARCHAR},
      </if>
      <if test="modifydate != null">
        #{modifydate,jdbcType=TIMESTAMP},
      </if>
      <if test="modifyname != null">
        #{modifyname,jdbcType=VARCHAR},
      </if>
      <if test="delFlag != null">
        #{delFlag,jdbcType=DECIMAL},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="com.cjrj.edu.entity.Stage">
    update T_STAGE
    <set>
      <if test="stageName != null">
        STAGE_NAME = #{stageName,jdbcType=VARCHAR},
      </if>
      <if test="createdate != null">
        CREATEDATE = #{createdate,jdbcType=TIMESTAMP},
      </if>
      <if test="createname != null">
        CREATENAME = #{createname,jdbcType=VARCHAR},
      </if>
      <if test="modifydate != null">
        MODIFYDATE = #{modifydate,jdbcType=TIMESTAMP},
      </if>
      <if test="modifyname != null">
        MODIFYNAME = #{modifyname,jdbcType=VARCHAR},
      </if>
      <if test="delFlag != null">
        DEL_FLAG = #{delFlag,jdbcType=DECIMAL},
      </if>
    </set>
    where STAGE_ID = #{stageId,jdbcType=DECIMAL}
  </update>
  
  <!-- 查询所有 -->
<select id="getStageList" resultMap="BaseResultMap">
select * from t_stage where  del_flag=0
</select>
</mapper>